<!DOCTYPE html>
<html>
    <head>
        <%- include('../partials/head') %>
    </head>

    <body>
        <header>
            <%- include('../partials/header') %>
        </header>
        <main>
            
            <% profiles.forEach(function(p){ %>
                <section>
                    <p><b>Name:</b> <%= p.name %></p>
                    <p><b>Telephone:</b><%= p.emergencyContactNumber %></p>
                    <a href="/editAccountAdmin?email=<%= p.email %>"><button>Edit Profile</button></a>
                </section>
            <% }) %>
            
            
        </main>
    </body>
</html>